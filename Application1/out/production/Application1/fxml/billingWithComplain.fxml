<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="720.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.BillingWithComplain">
   <children>
      <AnchorPane prefHeight="200.0" prefWidth="1080.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
      <AnchorPane layoutX="10.0" layoutY="10.0" prefHeight="520.0" prefWidth="1080.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0" spacing="50.0">
                     <children>
                        <TextField fx:id="complaintId" alignment="CENTER" prefHeight="50.0" prefWidth="300.0" promptText="COMPLAINT ID" />
                        <Button mnemonicParsing="false" onAction="#checkComplaint" prefHeight="50.0" prefWidth="200.0" text="CHECK" />
                     </children>
                  </HBox>
                  <ScrollPane prefHeight="300.0" prefWidth="200.0">
                     <content>
                        <TableView fx:id="tableView" prefHeight="1000.0" prefWidth="862.0">
                          <columns>
                            <TableColumn fx:id="tabId" prefWidth="84.0" text="ID" />
                            <TableColumn fx:id="tabMed" prefWidth="363.0" text="MEDICINE NAME" />
                              <TableColumn fx:id="tabQntty" prefWidth="174.0" text="QUANTITY" />
                              <TableColumn fx:id="tabPrice" minWidth="0.0" prefWidth="240.0" text="PRICE" />
                          </columns>
                        </TableView>
                     </content>
                  </ScrollPane>
                  <HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0" spacing="50.0">
                     <children>
                        <TextField fx:id="cusultaion" alignment="CENTER" prefHeight="50.0" prefWidth="200.0" promptText="CONSULTATION FEES(200)" />
                        <TextField fx:id="extra" alignment="CENTER" prefHeight="50.0" prefWidth="200.0" promptText="IF EXTRA" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0" spacing="50.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#okButton" prefHeight="50.0" prefWidth="200.0" text="OK" />
                        <Button mnemonicParsing="false" prefHeight="50.0" prefWidth="200.0" text="PRINT" />
                     </children>
                  </HBox>
               </children>
               <padding>
                  <Insets bottom="20.0" left="100.0" right="100.0" top="20.0" />
               </padding>
            </VBox>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
